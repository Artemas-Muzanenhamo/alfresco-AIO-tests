<extension>
  <modules>
    <module>
      <id>vanilla-alfresco-share-jar - Example Aikau Widgets</id>
      <version>1.0</version>
      <auto-deploy>true</auto-deploy>
      <configurations>
        <config evaluator="string-compare" condition="WebFramework" replace="false">
          <web-framework>
            <dojo-pages>
              <packages>
                <package name="tutorials" location="resources/vanilla-alfresco-share-jar/js/tutorials"/>
              </packages>
            </dojo-pages>
          </web-framework>
        </config>
      </configurations>

        <customizations>
            <!-- Custom toolbar for the document library -->
            <customization>
                <targetPackageRoot>org.alfresco.components.invite</targetPackageRoot>
                <sourcePackageRoot>org.alfresco.vanilla.components.invite</sourcePackageRoot>
            </customization>
        </customizations>
    </module>
  </modules>
</extension>